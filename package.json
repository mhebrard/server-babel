{
  "name": "Server",
  "version": "1.0.0",
  "description": "basic NodeJS server in ES6 with babel",
  "main": "index.js",
  "dependencies": {
    "babel-cli": "^6.26.0",
    "body-parser": "^1.17.2",
    "express": "^4.15.4",
    "graceful-fs": "^4.1.11"
  },
  "devDependencies": {
    "babel-preset-es2015": "^6.24.1",
    "nodemon": "^1.11.0",
    "xo": "^0.18.2"
  },
  "scripts": {
    "start": "nodemon src/index.js --exec babel-node --presets es2015",
    "build": "babel src -d dist --presets es2015",
    "serve": "node dist/index.js",
    "test": "xo"
  },
  "xo": {
    "space": 2,
    "env": [
      "browser",
      "node"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mhebrard/server-babel.git"
  },
  "keywords": [],
  "author": "Maxime Hebrard <maxime.hebrard@gmail.com> (http://github.com/mhebrard)",
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/mhebrard/server-babel/issues"
  },
  "homepage": "https://github.com/mhebrard/server-babel#readme"
}
